<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="104.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.masprojekt.PromptController">
   <children>
      <Text layoutX="5.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Czy chciałbyś dodać ten smartfon do listy smartfonów do odbioru ?" textAlignment="CENTER" wrappingWidth="450.0">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Button fx:id="yesButton" layoutX="14.0" layoutY="62.0" mnemonicParsing="false" onAction="#onYesButtonClick" prefHeight="27.0" prefWidth="95.0" text="Tak">
         <font>
            <Font name="System Bold" size="10.0" />
         </font>
      </Button>
      <Button fx:id="noButton" layoutX="345.0" layoutY="62.0" mnemonicParsing="false" onAction="#onNoButtonClick" prefHeight="27.0" prefWidth="95.0" text="Nie">
         <font>
            <Font name="System Bold" size="10.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
